{
  "name": "electron-todo",
  "version": "1.0.0",
  "description": "Simple todo app",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm build && tsc -p electron && electron-builder",
    "electron:dist": "npm build && tsc -p electron && electron-builder --mac --dir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krauchuk/electron-todo.git"
  },
  "author": "Ihar Krauchuk",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
